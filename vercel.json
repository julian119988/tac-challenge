{
  "version": 2,
  "builds": [
    {
      "src": "api/index.py",
      "use": "@vercel/python"
    }
  ],
  "routes": [
    {
      "src": "/health",
      "dest": "api/index.py"
    },
    {
      "src": "/health/ready",
      "dest": "api/index.py"
    },
    {
      "src": "/",
      "methods": ["GET"],
      "headers": {
        "Location": "/app"
      },
      "status": 308
    },
    {
      "src": "/",
      "methods": ["POST"],
      "dest": "api/index.py"
    },
    {
      "src": "/webhooks/github",
      "methods": ["POST"],
      "dest": "api/index.py"
    },
    {
      "src": "/app",
      "dest": "/apps/frontend/index.html"
    },
    {
      "src": "/(.*\\.(js|css|html|json|png|jpg|jpeg|gif|svg|mp4|webm|ico))",
      "dest": "/apps/frontend/$1"
    },
    {
      "src": "/(.*)",
      "dest": "api/index.py"
    }
  ],
  "headers": [
    {
      "source": "/apps/frontend/(.*\\.(css|js))",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "public, max-age=31536000, immutable"
        }
      ]
    },
    {
      "source": "/apps/frontend/(.*\\.html)",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "public, max-age=0, must-revalidate"
        }
      ]
    },
    {
      "source": "/apps/frontend/(.*\\.(mp4|webm|png|jpg|jpeg|gif|svg|ico))",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "public, max-age=604800"
        }
      ]
    },
    {
      "source": "/api/(.*)",
      "headers": [
        {
          "key": "Access-Control-Allow-Origin",
          "value": "*"
        },
        {
          "key": "Access-Control-Allow-Methods",
          "value": "GET, POST, OPTIONS"
        },
        {
          "key": "Access-Control-Allow-Headers",
          "value": "Content-Type, Authorization"
        }
      ]
    }
  ],
  "env": {
    "ENVIRONMENT": "production",
    "ADW_WORKING_DIR": "/tmp"
  }
}
