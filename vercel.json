{
  "version": 2,
  "builds": [
    {
      "src": "api/index.py",
      "use": "@vercel/python"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "api/index.py"
    }
  ],
  "env": {
    "GH_WB_SECRET": "@gh-wb-secret",
    "ANTHROPIC_API_KEY": "@anthropic-api-key",
    "ADW_WORKING_DIR": "/tmp",
    "SERVER_HOST": "0.0.0.0",
    "SERVER_PORT": "8000",
    "ENVIRONMENT": "production"
  }
}
